<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="@color/colorAltoGray" android:dashWidth="6dp" android:dashGap="4dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#80ffd300" android:endColor="#a0ffcb00" android:angle="270" android:centerY="0.75" android:centerColor="#80ffb600"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <corners android:radius="0dp"/>
                <stroke android:width="1dp" android:color="@color/color_purple_new" android:dashWidth="6dp" android:dashGap="4dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
